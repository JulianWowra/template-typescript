{
	"name": "",
	"description": "",
	"version": "",
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/dts/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"types": "./dist/dts/index.d.ts"
		}
	},
	"repository": "https://github.com/JulianWowra/template-typescript",
	"homepage": "https://julianwowra.github.io/template-typescript/",
	"author": "Julian Wowra <development@julianwowra.de>",
	"license": "MIT",
	"publishConfig": {
		"provenance": true
	},
	"packageManager": "pnpm@10.10.0",
	"devDependencies": {
		"@eslint/js": "9.25.1",
		"eslint": "9.25.1",
		"eslint-config-prettier": "10.1.2",
		"prettier": "3.5.3",
		"typedoc": "~0.28.3",
		"typedoc-github-theme": "~0.3.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.31.1"
	},
	"scripts": {
		"build": "pnpm run build:esm && pnpm run build:cjs && pnpm build:docs",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:docs": "typedoc src --plugin typedoc-github-theme",
		"lint": "pnpm lint:format && pnpm lint:code",
		"lint:format": "prettier --check .",
		"lint:code": "eslint ."
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"eslint"
		]
	},
	"keywords": []
}
